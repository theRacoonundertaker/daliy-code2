#include<iostream>
using namespace std;

int main() {
	for (int i = 0; i < 2; i++) { //outer loop: rows
		for (int j = 0; j < 3; j++) {//inner loop :columns
			cout << "*";
		}
		cout << endl;
	}
	cout << " " << endl;

	for (int i = 0; i < 2; i++) { //rows
		for (int j = 1; j < 6; j++) { //columns
			cout << j << " ";
		}//end of inner loop
		cout << endl; //inside outer loop but no inner loop!
	}

		cout << endl;


	for (int i = 0; i < 4; i++) { //rows
		for (int j = 0; j < 4; j++) { //columns
			cout << "*" << " ";
		}//end of inner loop
		cout << endl; //inside outer loop but no inner loop!
	}



}
